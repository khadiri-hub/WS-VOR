<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="VOR.Core.Domain" assembly="VOR.Core">
  <class name="TypeProgramme" table="TYPE_PROGRAMME">
    <id name="ID" type="Int32">
      <column name="ID_TYPE_PROGRAMME" sql-type="int" not-null="true" unique="true" />
      <generator class="identity" />
    </id>
    <property name="Code">
      <column name="CODE" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Description">
      <column name="DESCRIPTION" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>