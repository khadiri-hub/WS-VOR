    <id name="ID" type="Int32">
      <column name="PARK_TYPE_VEHICULE_ID" sql-type="int" not-null="true" unique="true" />
      <generator class="identity" />
    </id>
    <property name="Nom">
      <column name="PARK_TYPE_VEHICULE_NOM" sql-type="varchar" length="50" not-null="true" />
    </property>
    <property name="NomGb">
      <column name="PARK_TYPE_VEHICULE_NOM_GB" sql-type="varchar" length="50" not-null="true" />
    </property>
  </class>
</hibernate-mapping>